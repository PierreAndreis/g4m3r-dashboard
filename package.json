{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.28",
    "apollo-client": "^2.5.1",
    "classnames": "^2.2.6",
    "downshift": "^3.2.7",
    "emotion": "^9.2.12",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "jest-dom": "^3.1.2",
    "mdi-react": "^5.2.0",
    "mobx": "4.3.1",
    "mobx-react": "^5.4.3",
    "react": "^16.8.4",
